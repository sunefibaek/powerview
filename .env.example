# Eloverblik API Configuration
# Copy this file to .env and fill in your values

# Required: Eloverblik API refresh token
ELOVERBLIK_REFRESH_TOKEN=your-refresh-token-here

# Optional: Metering point metadata file (defaults to ./metering_points.yml)
METERING_POINTS_FILE=./metering_points.yml

# Optional: Storage and State Configuration (defaults shown)
DATA_STORAGE_PATH=./data
DUCKDB_PATH=./duckdb
STATE_DB_PATH=./state.duckdb

# Optional: Logging level (INFO, DEBUG, WARNING)
LOG_LEVEL=INFO

# Optional: Initial backfill period in days (default: 1095 = 3 years)
INITIAL_BACKFILL_DAYS=365
